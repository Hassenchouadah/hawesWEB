<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Pickup" table="pickup">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="iduser" type="integer" column="idUser" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="adressedepart" type="string" column="adresseDepart" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adressearrivee" type="string" column="adresseArrivee" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="heuredepart" type="string" column="heureDepart" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prix" type="string" column="prix" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
